{
  "name": "digital-security-reference",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "release": "release-it",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.0",
    "@docusaurus/plugin-content-docs": "2.0.0-beta.0",
    "@docusaurus/theme-classic": "2.0.0-beta.0",
    "@mdx-js/react": "^1",
    "@svgr/webpack": "^5",
    "buffer": "^6",
    "clsx": "^1",
    "crypto-browserify": "^3",
    "docusaurus-plugin-sass": "~0.2",
    "file-loader": "^6",
    "jose": "^3.12.2",
    "mermaid": "^8",
    "node-polyfill-webpack-plugin": "^1",
    "prop-types": "^15",
    "react": "^17",
    "react-dom": "^17",
    "rehype-katex": "^4",
    "remark-math": "^3",
    "sass": "^1.34.0",
    "stream-browserify": "^3",
    "url-loader": "^4"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^2",
    "release-it": "^14"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "requireCleanWorkingDir": true
    },
    "github": {
      "release": true,
      "releaseName": "Release ${version}",
      "tokenRef": "GITHUB_TOKEN"
    },
    "npm": {
      "publish": "false"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "refactor",
              "hidden": true
            }
          ]
        }
      }
    }
  }
}
